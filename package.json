{
  "name": "chai-html",
  "version": "2.0.1",
  "description": "HTML assertions for Chai.",
  "keywords": [
    "chai",
    "html",
    "test",
    "assert",
    "assertion"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "lib/chai-html.js",
  "scripts": {
    "test": "npm run verify && npm run spec",
    "spec": "mocha 'tests/spec/*.js'",
    "lint": "eslint .",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,yml}'",
    "verify": "npm run lint && npm run format",
    "coverage": "nyc --reporter=lcov --reporter=text npm run spec",
    "prepublishOnly": "npm run test && npm run build"
  },
  "author": "Matt Hinchliffe",
  "contributors": [
    "Daniel Elkington",
    "Kasper Pedersen"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:i-like-robots/chai-html.git"
  },
  "homepage": "https://github.com/i-like-robots/chai-html",
  "license": "MIT",
  "dependencies": {
    "deep-diff": "^1.0.0",
    "parse5": "^6.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.18.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2"
  }
}
