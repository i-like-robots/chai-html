{
  "name": "chai-html",
  "version": "2.0.1",
  "description": "HTML assertions for Chai.",
  "keywords": [
    "chai",
    "html",
    "test",
    "assert",
    "assertion"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "lib/chai-html.js",
  "scripts": {
    "test": "npm run lint && npm run spec",
    "spec": "mocha 'tests/spec/*.js'",
    "lint": "standard lib/*.js tests/spec/*.js",
    "coverage": "nyc node_modules/.bin/_mocha 'tests/spec/*.js'"
  },
  "author": "Matt Hinchliffe",
  "contributors": [
    "Daniel Elkington",
    "Kasper Pedersen"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:i-like-robots/chai-html.git"
  },
  "homepage": "https://github.com/i-like-robots/chai-html",
  "license": "MIT",
  "dependencies": {
    "deep-diff": "^1.0.0",
    "parse5": "^6.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.0",
    "mocha": "^8.3.0",
    "nyc": "^15.0.0",
    "standard": "^16.0.0"
  }
}
